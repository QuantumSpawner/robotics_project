Panels:
  - Class: rviz/Displays
    Name: Displays
    Property Tree Widget:
      Expanded:
        - /Navigation1
  - Class: rviz/Time
    Name: Time
  - Class: slam_toolbox::SlamToolboxPlugin
    Name: SlamToolboxPlugin
Visualization Manager:
  Class: ""
  Name: root
  Global Options:
    Fixed Frame: map
    Frame Rate: 30
  Displays:
  - Class: rviz/Grid
    Name: Grid
    Value: true
  - Class: rviz/RobotModel
    Name: RobotModel
    Description Source: Topic
    Description Topic:
      Value: /robot_description
    Value: true
  - Class: rviz/TF
    Name: TF
    Show Names: true
    Show Axes: true
    Show Arrows: true
    Filter (whitelist): "(map|odom|base_link)"
    Value: true
  - Class: rviz/Group
    Name: Sensors
    Enabled: false
    Displays:
    - Class: rviz/Odometry
      Name: Odometry
      Topic: odom
      Shape:
        Axes Length: 1
        Axes Radius: 0.1
        Head Length: 0.1
        Head Radius: 0.03
        Shaft Length: 0.3
        Shaft Radius: 0.015
        Value: Arrow
      Covariance:
        Orientation:
          Offset: 0.3
        Position:
          Alpha: 0.03
      Value: true
    - Class: rviz_imu_plugin/Imu
      Name: Imu
      Topic: cmp10a/imu
      Acceleration properties:
        Acc. vector scale: 0.1
        Enable acceleration: true
      Axes properties:
        Axes scale: 0.3
        Enable axes: true
      Value: true
    - Class: rviz/LaserScan
      Name: LaserScan
      Topic: hokuyo/scan
      Style: Points
      Color Transformer: AxisColor
      Axis: X
      Value: true
    - Class: rviz/Image
      Name: Image
      Image Topic: realsense/color/image_raw
      Value: true
    - Class: rviz/DepthCloud
      Name: DepthCloud
      Depth Map Topic: realsense/aligned_depth_to_color/image_raw
      Color Image Topic: realsense/color/image_raw
      Value: false
  - Class: rviz/Group
    Name: Navigation
    Enabled: true
    Displays:
    - Class: rviz/Map
      Name: Static Map
      Color Scheme: map
      Draw Behind: true
      Topic: map
      Value: true
    - Class: rviz/Map
      Name: Global Costmap
      Topic: move_base/global_costmap/costmap
      Color Scheme: costmap
      Draw Behind: true
      Value: true
    - Class: rviz/Map
      Name: Local Costmap
      Topic: move_base/local_costmap/costmap
      Color Scheme: costmap
      Draw Behind: false
      Value: true
    - Class: rviz/Path
      Name: Global Plan
      Topic: move_base/GlobalPlanner/plan
      Color: 25; 255; 0
      Value: true
    - Class: rviz/Path
      Name: Local Plan
      Topic: move_base/DWAPlannerROS/local_plan
      Color: 255; 0; 0
      Value: true
  - Class: rviz/Group
    Name: Slam
    Enabled: true
    Displays:
    - Class: rviz/InteractiveMarkers
      Name: Update
      Update Topic: /slam_toolbox/update
      Show Axes: true
      Show Descriptions: true
      Show Visual Aids: true
      Value: true
    - Class: rviz/MarkerArray
      Name: Path
      Marker Topic: /slam_toolbox/karto_graph_visualization
      Value: true
  Tools:
    - Class: rviz/Interact
    - Class: rviz/MoveCamera
    - Class: rviz/Select
    - Class: rviz/FocusCamera
    - Class: rviz/Measure
    - Class: rviz/SetGoal
      Topic: /move_base_simple/goal
  Views:
    Current:
      Class: rviz/TopDownOrtho
      Name: Current View
      Angle: -1.57
      Scale: 40
      Value: TopDownOrtho (rviz)
  Value: true
Window Geometry:
  Height: 1080
  Width: 1920
  QMainWindow State: 
